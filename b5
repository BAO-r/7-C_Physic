#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    float a,b,c;
    float delta,x1,x2,x;
    scanf("%f %f %f",&a,&b,&c);
    delta=b*b-4*a*c;
    if(delta>0)
    {
        x1=(-b+sqrt(delta))/(2*a);
        x2=(-b-sqrt(delta))/(2*a);
        printf("x1= %f\n",x1);
        printf("x2= %f\n",x2);
    }
    else if(delta==0)
    {
        x=-b/(2*a);
        printf("x= %f\n",x);
    }
    else
    {
        printf("vo nghiem");
    }

    return 0;
}
