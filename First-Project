#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    int i,j;
    int r=1;
    printf("DAY LA CHUONG TRINH DAU TAY CUA TOI\n");
    for(i=1; i<=7; i++)
    {
        for(j=0; j<=16-(r/2); j++)
        {
            printf(" ");
        }
        for(int a=r; a>0; a--)
        {
            printf("*");
        }
        if(i==1)
        {
            r+=2;
        }
        else
        {
            r+=4;
        }
        printf("\n");
    }
    for(i=1; i<=5; i++)
    {
        for(int m=1;m<=13;m++)
        {
            printf(" ");
        }
        for(int n=1; n<=9; n++)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
