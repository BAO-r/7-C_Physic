import java.util.Scanner;

public class Hello{
	public static void main(String args[]){
        Scanner scanner = new Scanner(System.in);
        System.out.println("Nhap so nguyen: ");
        int d = scanner.nextInt();
        int j = 1;
        for (int i = 1; i <= d; i++) {
            j = j * i;
        }
        System.out.println(d + "!= " + j);
    }
}
