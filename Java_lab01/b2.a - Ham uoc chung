import java.util.Scanner;

public class Hello{
	public static int UC(int n, int m) {
        int temp;
        while (m != 0) {
            temp = m;
            m = n % m;
            n = temp;
        }
        return n;
    }

    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Nhap so thu 1:");
        int n = sc.nextInt();
        System.out.println("Nhap so thu 2: ");
        int m = sc.nextInt();
        int a = UC(n, m);
        System.out.print("Uoc chung lon nhat cua " + n + " va " + m + " la: " + a);
        sc.close();
    }
}
